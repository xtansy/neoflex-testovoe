{
    "version": 2,
    "builds": [
        {
            "src": "src/index.html",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/.*",
            "dest": "http://localhost:3001"
        },
        {
            "src": "/(.*)",
            "dest": "src/index.html"
        }
    ]
}